{
  "name": "hanks-tank-backend",
  "version": "2.0.0",
  "description": "MLB Analytics Backend API using MLB StatsAPI",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "npx tsc && node dist/app.js",
    "build": "npx tsc --project tsconfig.json",
    "gcp-build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "serve": "npm run build && npm start",
    "clean": "rm -rf dist",
    "type-check": "npx tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && gcloud app deploy --quiet",
    "deploy:prod": "npm run build && gcloud app deploy --promote --quiet",
    "gcp:logs": "gcloud app logs tail -s default",
    "gcp:browse": "gcloud app browse"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahcraig45/mlb_pi.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elijahcraig45/mlb_pi/issues"
  },
  "homepage": "https://github.com/elijahcraig45/mlb_pi#readme",
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.4",
    "@google-cloud/storage": "^7.17.0",
    "@types/axios": "^0.9.36",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.1",
    "fs": "^0.0.1-security",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}
